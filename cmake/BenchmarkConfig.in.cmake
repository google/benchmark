get_filename_component(Benchmark_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
if(NOT TARGET google.benchmark AND EXISTS "${Benchmark_CMAKE_DIR}/BenchmarkTargets.cmake")
  include("${Benchmark_CMAKE_DIR}/BenchmarkTargets.cmake")
  get_filename_component(_Benchmark_PREFIX "${Benchmark_CMAKE_DIR}" PATH)
  get_filename_component(_Benchmark_PREFIX "${_Benchmark_PREFIX}" PATH)
  get_filename_component(_Benchmark_PREFIX "${_Benchmark_PREFIX}" PATH)
  get_target_property(incdirs google.benchmark INTERFACE_INCLUDE_DIRECTORIES)
  if(NOT incdirs)
    unset(incdirs)
  endif()
  list(APPEND incdirs "${_Benchmark_PREFIX}/include")
  set_target_properties(google.benchmark PROPERTIES INTERFACE_INCLUDE_DIRECTORIES "${incdirs}")
endif()
set(GOOGLE_BENCHMARK_ENABLE_MPI @BENCHMARK_ENABLE_MPI@)
